@model List<KeepPasswords.Models.Calendar.CalendarItem>


<div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">События на @ViewBag.Date</h5>
            <button type="button" class="btn-close" onclick="globalCloseModal()"></button>
        </div>
        <div class="modal-body">
           @foreach(var item in Model)
            {
                <span>@item.EventName - @item.Date.ToString("dd.MM.yyyy")</span>
            }



        </div>
      
    </div>
</div>